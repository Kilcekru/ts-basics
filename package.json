{
	"name": "@kilcekru/ts-basics",
	"version": "4.0.0",
	"description": "Configuration for typescript and eslint",
	"keywords": [
		"typescript",
		"tsconfig",
		"eslint"
	],
	"license": "ISC",
	"author": {
		"name": "kilcekru",
		"email": "kilcekru@unkreativ.at"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Kilcekru/ts-basics"
	},
	"homepage": "https://github.com/Kilcekru/ts-basics/tree/main#readme",
	"bugs": "https://github.com/Kilcekru/ts-basics/issues",
	"engines": {
		"node": ">=20.11"
	},
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"types": "./index.d.ts",
			"default": "./index.js"
		},
		"./tsconfig-*.json": "./tsconfig-*.json"
	},
	"files": [
		"eslint/**/*",
		"index.js",
		"tsconfig-*.json"
	],
	"scripts": {
		"clean:all": "rm -rf node_modules",
		"lint": "eslint .",
		"prepack": "pnpm lint"
	},
	"dependencies": {
		"@eslint/js": "9.22.0",
		"eslint-config-prettier": "10.1.1",
		"eslint-plugin-import": "2.31.0",
		"eslint-plugin-react": "7.37.4",
		"eslint-plugin-react-hooks": "5.2.0",
		"eslint-plugin-simple-import-sort": "12.1.1",
		"eslint-plugin-solid": "0.14.5",
		"globals": "16.0.0",
		"typescript-eslint": "8.26.0"
	},
	"devDependencies": {
		"eslint": "9.22.0",
		"typescript": "5.8.2"
	},
	"peerDependencies": {
		"eslint": "^9",
		"typescript": ">=5.4 <5.9"
	}
}
