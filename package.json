{
	"name": "@kilcekru/ts-basics",
	"version": "4.0.0-next.0",
	"description": "Configuration for typescript and eslint",
	"keywords": [
		"typescript",
		"tsconfig",
		"eslint"
	],
	"license": "ISC",
	"author": {
		"name": "kilcekru",
		"email": "kilcekru@unkreativ.at"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Kilcekru/ts-basics"
	},
	"homepage": "https://github.com/Kilcekru/ts-basics/tree/main#readme",
	"bugs": "https://github.com/Kilcekru/ts-basics/issues",
	"engines": {
		"node": ">=20.11"
	},
	"sideEffects": false,
	"type": "module",
	"main": "index.js",
	"types": "index.d.ts",
	"exports": {
		".": {
			"types": "./index.d.ts",
			"default": "./index.js"
		}
	},
	"files": [
		"eslint/**/*",
		"index.js",
		"tsconfig-*.json"
	],
	"scripts": {
		"clean:all": "rm -rf node_modules"
	},
	"dependencies": {
		"@eslint/js": "9.15.0",
		"eslint": "9.15.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-import": "2.31.0",
		"eslint-plugin-react": "7.37.2",
		"eslint-plugin-react-hooks": "5.0.0",
		"eslint-plugin-simple-import-sort": "12.1.1",
		"eslint-plugin-solid": "^0.14.4",
		"globals": "15.12.0",
		"typescript-eslint": "8.15.0"
	},
	"devDependencies": {
		"typescript": "5.6.3"
	},
	"peerDependencies": {
		"typescript": ">=5.4 <5.7"
	}
}
